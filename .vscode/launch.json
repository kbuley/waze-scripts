{
	"version": "0.2.0",
	"configurations": [
        {
          "name" : "Launch Waze",
          "type"  : "chrome",
          "request" : "launch",
          "url" : "http://www.waze.com/editor/?",
          "runtimeArgs": ["--remote-debugging-port=9222","--new-window"]
        },
		{
			"name": "Launch index.html",
			"type": "chrome",
			"request": "launch",
			"file": "${workspaceRoot}/index.html"
		},
		{
			"name": "Launch localhost with sourcemaps",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost/mypage.html",
			"sourceMaps": true,
			"webRoot": "wwwroot"
		},
		{
			"name": "Attach",
			"type": "chrome",
			"request": "attach",
			"port": 9222
		}
	]
}